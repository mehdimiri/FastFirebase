<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Description>Send push notification by Firebase</Description>
    <PackageProjectUrl>https://github.com/mehdimiri/FastFirebase</PackageProjectUrl>
    <RepositoryUrl>https://github.com/mehdimiri/FastFirebase</RepositoryUrl>
    <PackageTags>push,push-notifications,firebase,asp.net,core</PackageTags>
    <PackageLicenseExpression></PackageLicenseExpression>
    <Authors>Mehdi Miri</Authors>
    <Company />
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <Version>1.0.3</Version>
    <PackageIcon>icons8-firebase-48.png</PackageIcon>
    <PackageIconUrl />
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.Options" Version="5.0.0" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\icons8-firebase-48.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
    <None Include="LICENSE.txt">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
